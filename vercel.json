{
  "version": 2,
  "name": "urban-indumentaria",
  "builds": [
    {
      "src": "nextjs-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ]
    }
  ],
  "env": {
    "NEXT_PUBLIC_SITE_NAME": "Urban Indumentaria"
  },
  "regions": ["gru1"],
  "framework": "nextjs"
}
